{
  "version": 2,
  "builds": [
    {
      "src": "backend/supabase-server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/supabase-server.js"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ],
}